{
  "private": true,
  "name": "preact",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "preact build",
    "serve": "sirv build --port 8080 --cors --single",
    "dev": "preact watch",
    "lint": "eslint src",
    "test": "jest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^7.0.0-alpha.29",
    "@storybook/addon-coverage": "^0.0.4",
    "@storybook/addon-docs": "^7.0.0-alpha.29",
    "@storybook/addon-essentials": "^7.0.0-alpha.29",
    "@storybook/addon-interactions": "^7.0.0-alpha.29",
    "@storybook/addon-links": "^7.0.0-alpha.29",
    "@storybook/preact-webpack5": "^7.0.0-alpha.29",
    "@storybook/test-runner": "^0.7.0",
    "@storybook/testing-library": "^0.0.14-next.0",
    "babel-loader": "^8.2.5",
    "core-js": "^3.22.8",
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "eslint": "^6.0.1",
    "eslint-config-preact": "^1.1.0",
    "jest-preset-preact": "^1.0.0",
    "preact-cli": "^3.0.0",
    "sirv-cli": "1.0.3",
    "storybook": "^7.0.0-alpha.29"
  },
  "dependencies": {
    "preact": "^10.3.2",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  },
  "packageManager": "yarn@3.2.1"
}
