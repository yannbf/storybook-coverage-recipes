    "@storybook/builder-vite": "^7.0.0-alpha.29",
    "@storybook/test-runner": "^0.7.0",
    "@storybook/testing-library": "^0.0.14-next.0",
    "babel-loader": "^8.2.5",
    "storybook": "^7.0.0-alpha.29",
    "typescript": "~4.7.2",
    "vite": "^3.0.4",
    "vite-plugin-istanbul": "^2.9.0"
  }
